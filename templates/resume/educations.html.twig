        <article class="timeline__item">
            <h4 class="title title--h4 timeline__title">{{ education.school }} - {{ education.title }}</h4>
            <span class="timeline__period">{{ education.startDate|date('F Y') }} â€” {{ education.endDate|date('F Y') }}</span>
            <p class="timeline__description">{{ education.description }}</p>
        </article>
    {% endfor %}
</div>
